<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="200.0" prefWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NewFileController">
    <padding>
        <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
    </padding>
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Create a file:" />
      <TextField fx:id="textField" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" />
        <Button fx:id="createBtn" mnemonicParsing="false" onAction="#submit" text="Create" />
        <Text fx:id="actionText" strokeType="OUTSIDE" strokeWidth="0.0" />
    </children>
</VBox>
